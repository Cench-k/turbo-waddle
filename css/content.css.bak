@charset "UTF-8";

@font-face { font-family: 'BMDOHYEON'; src: url('https://cdn.jsdelivr.net/gh/projectnoonnu/noonfonts_one@1.0/BMDOHYEON.woff') format('woff'); font-weight: normal; font-style: normal; }

body { margin:0; padding:0; }

	

	.wrap { overflow:hidden; position:relative; }
		.slide_box { margin:0 auto; position:relative; }
			.box { width:100%; position:relative; }
				.port { width:100%; height:90%; position:absolute; left:0; top:0; overflow:hidden; text-align:center; }

				

				.view { display:none; }

					.title { display:inline-block; position:absolute; top:0; left:50%; margin-left:-70px; }

					.list_area { width:100%; height:1400px; box-sizing:border-box; padding:3% 0; overflow-y:auto; position:absolute; }

					.list_div { width:100%; margin:auto; overflow:auto; position:absolute;
								 -ms-overflow-style: none;
								}
					.list_div::-webkit-scrollbar {
									display: none;
								 }

					.test { width:30%; height:45%; background:#fff; display:inline-block; float:left; margin-left:5%; margin-top:5%; box-sizing:border-box; padding:1% 2%; position:relative;
							transition: 0.5s all ease;
							-webkit-transition: 0.5s all ease;
							-moz-transition: 0.5s all ease;
							-ms-transition: 0.5s all ease;
							position:relative;
							box-sizing:border-box;
							border:1px solid #292929;
							text-align:left;
							}
					
					.back { width:100%; height:45%; float:left; display:inline-block; }

					.test:nth-of-type(3n+1) { margin-left:0; }
						
						.test > img { display:inline-block; width:100%; height:60%; }
						

				.thum_box { width:100%; height:10%; position:absolute; bottom:0; left:0; text-align:center; }
					.bottom_list { display:inline-block; width:100%; height:100%; padding:0; margin:0; }
					.bottom_list > li { float:left; width:20%; height:100%; list-style:none; border:0; font-family:"BMDOHYEON"; background:#fff; }
					.bottom_list > li.on { background:#232323; color:#fff; }

				

				.prev { width:2%; height:5%; position:absolute; left:0; top:50%; margin-top:-5%; background:#efefef; z-index:15; }
				.next { width:2%; height:5%; position:absolute; right:0; top:50%; margin-top:-5%; background:#ededed; z-index:15; }
				
				.pro_table td { padding:10px; text-align:left; }

				.skill_con { float:left; width:20%; padding:10px; box-sizing:border-box; text-align:center; border-left:2px solid #dadada; position:relative; }

				.img_on1 { width:614px; height:418px; text-align:center; z-index:5; display:none; }
				.img_on2 { width:912px; height:408px; text-align:center; z-index:5; display:none; background:url('img/Academic.png') no-repeat center top; background-size:contain; }
				.img_on2 > img { float:left; }
/* 화면크기가 1030px보다 클 때 */
@media screen and (min-width:1500px)  {
	.wrap { width:100%; max-width:1920px; min-width:1500px; margin:0 auto; }
		.slide_box { width:100%; }
		.bottom_list > li { line-height:320%; font-size:30px; }
		.list_area { left:-228px; top:0;
					 transform: skewX(-40deg);
					 -ms-transform: skewX(-40deg);
					 -moz-transform: skewX(-40deg);
					 -webkit-transform: skewX(-40deg);
					 -o-transform: skewX(-40deg);
					}
		.list_div { width:75%; height:45%; padding:0 13% 0 12%; }
			.test:hover { 
							transform: skewX(40deg);
							-ms-transform: skewX(40deg);
							-moz-transform: skewX(40deg);
							-webkit-transform: skewX(40deg);
							-o-transform: skewX(40deg);
							z-index:10;
							box-shadow:65px 40px 25px -40px rgba(0,0,0,0.2)
							}
			.test > p { font-size:22px; font-weight:bold; font-family:"BMDOHYEON"; }
				.go_site_btn { right:15px; bottom:10px; text-align:center; }
					.link_text { display:none; }

			.pro_table { margin-top:5%; }
			.pro_table td { border:1px solid #dadada; font-family:"BMDOHYEON"; }

			.img_on1 { position:absolute; left:50px; top:-130px; }
			.img_on2 { position:absolute; left:-500px; top:-130px; }
}
/*중간 크기*/
@media screen and (min-width:1200px)  and  (max-width:1499px){
	.wrap { width:100%; max-width:1920px; min-width:1200px; margin:0 auto; }
		.slide_box { width:100%; }
		.bottom_list > li { line-height:260%; font-size:25px; }
		.list_area { left:-327px; top:0;
					 transform: skewX(-40deg);
					 -ms-transform: skewX(-40deg);
					 -moz-transform: skewX(-40deg);
					 -webkit-transform: skewX(-40deg);
					 -o-transform: skewX(-40deg);
					}
		.list_div { width:75%; height:36%; padding:0 13% 0 12%; }
			.test:hover { 
							transform: skewX(40deg);
							-ms-transform: skewX(40deg);
							-moz-transform: skewX(40deg);
							-webkit-transform: skewX(40deg);
							-o-transform: skewX(40deg);
							z-index:10;
							box-shadow:65px 40px 25px -40px rgba(0,0,0,0.2)
							}
			.test > p { font-size:18px; font-weight:bold; font-family:"BMDOHYEON"; }
				.go_site_btn { right:15px; bottom:10px; text-align:center; }
					.link_text { display:none; }

			.pro_table { margin-top:5%; }
			.pro_table td { border:1px solid #dadada; font-family:"BMDOHYEON"; }

			.img_on1 { position:absolute; left:50px; top:-130px; z-index:5; }
			.img_on2 { position:absolute; left:50px; top:-130px; z-index:5; }
}
/* 화면크기가 1030px보다 작을 때 */
@media screen and (max-width:1200px) {
	.wrap { width:100%; max-width:1200px; min-width:640px; margin:0 auto; }
		.slide_box { width:100%; }
		.bottom_list > li { line-height:300%; font-size:22px; }
		.list_area { left:-90px; top:360px;
					 transform: skewX(-17deg);
					 -ms-transform: skewX(-17deg);
					 -moz-transform: skewX(-17deg);
					 -webkit-transform: skewX(-17deg);
					 -o-transform: skewX(-17deg);
					}
		.list_div { width:75%; height:60%; padding:0 13% 0 14%; } 
			.test { height:20%; }
			.back { height:20%; }
			.test:hover { 
						transform: skewX(17deg);
						-ms-transform: skewX(17deg);
						-moz-transform: skewX(17deg);
						-webkit-transform: skewX(17deg);
						-o-transform: skewX(17deg);
						z-index:10
						box-shadow:65px 40px 25px -40px rgba(0,0,0,0.2);
						}
			.test > p { font-size:15px; font-weight:bold; font-family:"BMDOHYEON"; }
				.go_site_btn { right:15px; bottom:10px; text-align:center; }
					.link_text { display:none; font-size:11px; }

			.pro_table { margin-top:10%; }
			.pro_table td { border:1px solid #dadada; font-family:"BMDOHYEON"; font-size:13px; }

			.img_on1 { position:absolute; left:-105px; top:-220px; }
			.img_on2 { width:600px; height:300px; position:absolute; left:-45px; top:-220px; }
}

.go_site_btn { width:11%; height:15%; position:absolute; border:1px solid #292929; cursor:pointer; border-radius:15%;
				/*
				animation-name: go_site;
				animation-duration: 1s;
				animation-delay:.5s;
				animation-iteration-count:infinite;
				animation-direction: cubic-bezier(.17,.67,.16,.67) altenate;
				*/
				}

@keyframes go_site{
	0% { border-radius:0; }
	25% { border-radius:10% }
	50% { border-radius:20% }
	75% { border-radius:40% }
	100% { border-radius:50%; }
}

-webkit-@keyframes go_site{
	0% { border-radius:0; }
	25% { border-radius:10% }
	50% { border-radius:20% }
	75% { border-radius:40% }
	100% { border-radius:50%; }
}

-moz-@keyframes go_site{
	0% { border-radius:0; }
	25% { border-radius:10% }
	50% { border-radius:20% }
	75% { border-radius:40% }
	100% { border-radius:50%; }
}

-ms-@keyframes go_site{
	0% { border-radius:0; }
	25% { border-radius:10% }
	50% { border-radius:20% }
	75% { border-radius:40% }
	100% { border-radius:50%; }
}

-o-@keyframes go_site{
	0% { border-radius:0; }
	25% { border-radius:10% }
	50% { border-radius:20% }
	75% { border-radius:40% }
	100% { border-radius:50%; }
}